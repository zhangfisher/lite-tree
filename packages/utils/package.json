{
  "name": "@lite-tree/utils",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "src/index.ts",  
  "homepage": "https://github.com/zhangfisher/lite-tree",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhangfisher/lite-tree"
  },
  "exports": {
    ".": {
      "default": "./src/index.ts"      
    },
    "./*": {
      "default": "./src/*.ts",
      "import": "./src/*.ts",
      "types": "./src/*.ts" 
    }   
  },
  "publishConfig": {
    "access": "public",
    "main":"dist/index.js",
    "module":"dist/index.mjs",
    "types":"dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js",
        "types":"./dist/index.d.ts"
      },
      "./*": {
        "import": "./dist/*.mjs",
        "require": "./dist/*.js",
        "types"   :"./dist/*.d.ts"
      }   
    } 
  },
  "scripts": {
    "build": "tsup"
  },
  "keywords": [
    "lite-tree"
  ],
  "author": "zhangfisher",
  "license": "MIT"
}
